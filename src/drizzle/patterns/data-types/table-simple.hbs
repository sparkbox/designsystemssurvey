<table class="cmp-basic-table" data-simple data-theme="{{ chartTheme }}">
  <tr>
    <th>Categories</th>
    <th>Percentage of Replies</th>
  </tr>
  {{#each chartData}}
    <tr>
      <td class="table__category">{{ this.category }}</td>
      <td><span class="table__value">{{ this.value }}</span><span>%</span></td>
    </tr>
  {{/each}}
</table>
